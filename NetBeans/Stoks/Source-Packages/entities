package entities;

public class StockProduct {

    long stock_id;
    long product_id;
    String date;
    long qt;
    String flag_min_qt;

    public StockProduct() {
    }

    public StockProduct(long stock_id, long product_id, String date, long qt, String flag_min_qt) {
        this.stock_id = stock_id;
        this.product_id = product_id;
        this.date = date;
        this.qt = qt;
        this.flag_min_qt = flag_min_qt;
    }

    public long getStock_id() {
        return stock_id;
    }

    public void setStock_id(long stock_id) {
        this.stock_id = stock_id;
    }

    public long getProduct_id() {
        return product_id;
    }

    public void setProduct_id(long product_id) {
        this.product_id = product_id;
    }

    public String getDate() {
        return date;
    }

    public void setDate(String date) {
        this.date = date;
    }

    public long getQt() {
        return qt;
    }

    public void setQt(long qt) {
        this.qt = qt;
    }

    public String getFlag_min_qt() {
        return flag_min_qt;
    }

    public void setFlag_min_qt(String flag_min_qt) {
        this.flag_min_qt = flag_min_qt;
    }

    @Override
    public String toString() {
        return "StockProduct{" + "stock_id=" + stock_id + ", product_id=" + product_id + ", date=" + date + ", qt=" + qt + ", flag_min_qt=" + flag_min_qt + '}';
    }
    
    
}



package entities;

public class Supplier {

    long id;
    String name;
    String phone;
    String adress;
    String email;

    public Supplier() {
    }

    public Supplier(long id, String name, String phone, String adress, String email) {
        this.id = id;
        this.name = name;
        this.phone = phone;
        this.adress = adress;
        this.email = email;
    }

    public long getId() {
        return id;
    }

    public void setId(long id) {
        this.id = id;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getPhone() {
        return phone;
    }

    public void setPhone(String phone) {
        this.phone = phone;
    }

    public String getAdress() {
        return adress;
    }

    public void setAdress(String adress) {
        this.adress = adress;
    }

    public String getEmail() {
        return email;
    }

    public void setEmail(String email) {
        this.email = email;
    }

    @Override
    public String toString() {
        return "Supplier{" + "id=" + id + ", name=" + name + ", phone=" + phone + ", adress=" + adress + ", email=" + email + '}';
    }
    
    
}



package entities;

public class Transfer {

    long stock_id;
    long vehicule_id;
    long product_id;
    String date;
    long qt;

    public Transfer() {
    }

    public Transfer(long stock_id, long vehicule_id, long product_id, String date, long qt) {
        this.stock_id = stock_id;
        this.vehicule_id = vehicule_id;
        this.product_id = product_id;
        this.date = date;
        this.qt = qt;
    }

    public long getStock_id() {
        return stock_id;
    }

    public void setStock_id(long stock_id) {
        this.stock_id = stock_id;
    }

    public long getVehicule_id() {
        return vehicule_id;
    }

    public void setVehicule_id(long vehicule_id) {
        this.vehicule_id = vehicule_id;
    }

    public long getProduct_id() {
        return product_id;
    }

    public void setProduct_id(long product_id) {
        this.product_id = product_id;
    }

    public String getDate() {
        return date;
    }

    public void setDate(String date) {
        this.date = date;
    }

    public long getQt() {
        return qt;
    }

    public void setQt(long qt) {
        this.qt = qt;
    }

    @Override
    public String toString() {
        return "Transfer{" + "stock_id=" + stock_id + ", vehicule_id=" + vehicule_id + ", product_id=" + product_id + ", date=" + date + ", qt=" + qt + '}';
    }
    
    
            
}



package entities;

public class User {

    long id;
    String username;
    String userpass;
    String valid;
    long employee_id;

    public User() {
    }

    public User(long id, String username, String userpass, String valid, long employee_id) {
        this.id = id;
        this.username = username;
        this.userpass = userpass;
        this.valid = valid;
        this.employee_id = employee_id;
    }

    public long getId() {
        return id;
    }

    public void setId(long id) {
        this.id = id;
    }

    public String getUsername() {
        return username;
    }

    public void setUsername(String username) {
        this.username = username;
    }

    public String getUserpass() {
        return userpass;
    }

    public void setUserpass(String userpass) {
        this.userpass = userpass;
    }

    public String getValid() {
        return valid;
    }

    public void setValid(String valid) {
        this.valid = valid;
    }

    public long getEmployee_id() {
        return employee_id;
    }

    public void setEmployee_id(long employee_id) {
        this.employee_id = employee_id;
    }

    @Override
    public String toString() {
        return "User{" + "id=" + id + ", username=" + username + ", userpass=" + userpass + ", valid=" + valid + ", employee_id=" + employee_id + '}';
    }
    
    
}



package entities;

public class Vehicule {

    long id;
    String name;
    long stock_id;

    public Vehicule() {
    }

    public Vehicule(long id, String name, long stock_id) {
        this.id = id;
        this.name = name;
        this.stock_id = stock_id;
    }

    public long getId() {
        return id;
    }

    public void setId(long id) {
        this.id = id;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public long getStock_id() {
        return stock_id;
    }

    public void setStock_id(long stock_id) {
        this.stock_id = stock_id;
    }

    @Override
    public String toString() {
        return "Vehicule{" + "id=" + id + ", name=" + name + ", stock_id=" + stock_id + '}';
    }
    
    
}
