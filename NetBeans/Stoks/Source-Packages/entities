package entities;
public class ProductSupplier {
    
    long product_id;
    long supplier_id;
    String date;
    long price_unite;

    public ProductSupplier() {
    }

    public ProductSupplier(long product_id, long supplier_id, String date, long price_unite) {
        this.product_id = product_id;
        this.supplier_id = supplier_id;
        this.date = date;
        this.price_unite = price_unite;
    }

    public long getProduct_id() {
        return product_id;
    }

    public void setProduct_id(long product_id) {
        this.product_id = product_id;
    }

    public long getSupplier_id() {
        return supplier_id;
    }

    public void setSupplier_id(long supplier_id) {
        this.supplier_id = supplier_id;
    }

    public String getDate() {
        return date;
    }

    public void setDate(String date) {
        this.date = date;
    }

    public long getPrice_unite() {
        return price_unite;
    }

    public void setPrice_unite(long price_unite) {
        this.price_unite = price_unite;
    }

    @Override
    public String toString() {
        return "ProductSupplier{" + "product_id=" + product_id + ", supplier_id=" + supplier_id + ", date=" + date + ", price_unite=" + price_unite + '}';
    }
    
}



