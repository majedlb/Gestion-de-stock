        this.name_type = name_type;
        this.desc_type = desc_type;
    }

    public long getId() {
        return id;
    }

    public void setId(long id) {
        this.id = id;
    }

    public String getName_type() {
        return name_type;
    }

    public void setName_type(String name_type) {
        this.name_type = name_type;
    }

    public String getDesc_type() {
        return desc_type;
    }

    public void setDesc_type(String desc_type) {
        this.desc_type = desc_type;
    }

    @Override
    public String toString() {
        return "EmployeeType{" + "id=" + id + ", name_type=" + name_type + ", desc_type=" + desc_type + '}';
    }
    
    
}



package entities;

public class Invoice {

    long id;
    String name;
    String date;
    long disc;
    long stock_id;
    long supplier_id;

    public Invoice() {
    }

    public Invoice(long id, String name, String date, long disc, long stock_id, long supplier_id) {
        this.id = id;
        this.name = name;
        this.date = date;
        this.disc = disc;
        this.stock_id = stock_id;
        this.supplier_id = supplier_id;
    }

    public long getId() {
        return id;
    }

    public void setId(long id) {
        this.id = id;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getDate() {
        return date;
    }

    public void setDate(String date) {
        this.date = date;
    }

    public long getDisc() {
        return disc;
    }

    public void setDisc(long disc) {
        this.disc = disc;
    }

    public long getStock_id() {
        return stock_id;
    }

    public void setStock_id(long stock_id) {
        this.stock_id = stock_id;
    }

    public long getSupplier_id() {
        return supplier_id;
    }

    public void setSupplier_id(long supplier_id) {
        this.supplier_id = supplier_id;
    }

    @Override
    public String toString() {
        return "Invoice{" + "id=" + id + ", name=" + name + ", date=" + date + ", disc=" + disc + ", stock_id=" + stock_id + ", supplier_id=" + supplier_id + '}';
    }
    
    
}



package entities;

public class InvoiceProduct {
long product_id;
long invoice_id;
String date;
long priceunite;
long qt;
long disc;

    public InvoiceProduct() {
    }

    public InvoiceProduct(long product_id, long invoice_id, String date, long priceunite, long qt, long disc) {
        this.product_id = product_id;
        this.invoice_id = invoice_id;
        this.date = date;
        this.priceunite = priceunite;
        this.qt = qt;
        this.disc = disc;
    }

    public long getProduct_id() {
        return product_id;
    }

    public void setProduct_id(long product_id) {
        this.product_id = product_id;
    }

    public long getInvoice_id() {
        return invoice_id;
    }

    public void setInvoice_id(long invoice_id) {
        this.invoice_id = invoice_id;
    }

    public String getDate() {
        return date;
    }

    public void setDate(String date) {
        this.date = date;
    }

    public long getPriceunite() {
        return priceunite;
    }

    public void setPriceunite(long priceunite) {
        this.priceunite = priceunite;
    }

    public long getQt() {
        return qt;
    }

    public void setQt(long qt) {
        this.qt = qt;
    }

    public long getDisc() {
        return disc;
    }

    public void setDisc(long disc) {
        this.disc = disc;
    }

    @Override
    public String toString() {
        return "InvoiceProduct{" + "product_id=" + product_id + ", invoice_id=" + invoice_id + ", date=" + date + ", priceunite=" + priceunite + ", qt=" + qt + ", disc=" + disc + '}';
    }
 
}



